{
  "annotations": { "list": [] },
  "panels": [
    {
      "type": "stat",
      "title": "RL Decisions (Blocks - 5m)",
      "targets": [
        {
          "expr": "sum(increase(instainstru_rl_decisions_total{action=\"block\"}[5m]))"
        }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "RL Decisions (Shadow Blocks - 5m)",
      "targets": [
        {
          "expr": "sum(increase(instainstru_rl_decisions_total{action=\"shadow_block\"}[5m]))"
        }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Retry-After p95 (seconds)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(instainstru_rl_retry_after_seconds_bucket[5m])))"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }
    }
  ],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "title": "Rate Limiting Overview",
  "version": 1
}
