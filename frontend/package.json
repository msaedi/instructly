{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:http": "rm -rf .next && NEXT_PUBLIC_API_URL=http://localhost:8000 next dev",
    "dev:https": "rm -rf .next && cross-env NEXT_PUBLIC_API_URL=https://localhost:8001 NEXT_PUBLIC_APP_URL=https://localhost:3001 node server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:working": "playwright test e2e/tests/smoke.spec.ts e2e/tests/basic-search.spec.ts e2e/tests/example-working.spec.ts",
    "test:e2e:ci": "playwright test e2e/tests/smoke.spec.ts e2e/tests/example-working.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "playwright:install": "playwright install"
  },
  "dependencies": {
    "@stripe/stripe-js": "^7.4.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
