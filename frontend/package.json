{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "audit:deps": "depcruise --config dependency-cruiser.config.cjs --ts-config tsconfig.json app components features hooks lib | jq '.'",
    "audit:deps:ci": "depcruise --config dependency-cruiser.config.cjs --ts-config tsconfig.json app components features hooks lib --output-type err",
    "audit:exports": "ts-prune -p tsconfig.json -i \"(^\\.next/|^e2e/|^types/generated/|\\.d\\.ts$|^playwright\\.config\\.ts$|^next\\.config\\.ts$|^middleware\\.ts$|^app/|^components/|^hooks/|^legacy-patterns/)\" | grep -v 'used in module' | grep -v -f ts-prune-allowlist.txt || true",
    "audit:exports:ci": "mkdir -p .artifacts && ts-prune -p tsconfig.json -i \"(^\\.next/|^e2e/|^types/generated/|\\.d\\.ts$|^playwright\\.config\\.ts$|^next\\.config\\.ts$|^middleware\\.ts$|^app/|^components/|^hooks/|^legacy-patterns/)\" | grep -v 'used in module' | grep -v -f ts-prune-allowlist.txt | tee .artifacts/ts-prune.txt; test ! -s .artifacts/ts-prune.txt",
    "dev": "next dev",
    "dev:test": "next dev -p 3100",
    "dev:http": "rm -rf .next && NEXT_PUBLIC_API_BASE=http://localhost:8000 next dev",
    "dev:https": "rm -rf .next && cross-env NEXT_PUBLIC_API_BASE=https://localhost:8001 NEXT_PUBLIC_APP_URL=https://localhost:3001 node server.js",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ci": "next lint --max-warnings=0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit",
    "typecheck:strict:log": "npm run typecheck:strict 2>&1 | tee .artifacts/ts-noUncheckedIndexedAccess.log",
    "typecheck:strict-all": "tsc -p tsconfig.strict-all.json --noEmit",
    "typecheck:strict-all:log": "npm run typecheck:strict-all 2>&1 | tee .artifacts/ts-strict-all.log",
    "typecheck:strict-all:fast": "tsc -p tsconfig.strict-all.json --noEmit --pretty false --incremental --tsBuildInfoFile .cache/tsbuildinfo.strict",
    "build:log": "mkdir -p .artifacts && bash -o pipefail -c \"NEXT_TELEMETRY_DISABLED=1 next build 2>&1 | tee .artifacts/next-build.log\"",
    "build:warn-check": "node scripts/check-next-warnings.js",
    "verify:runtime-validation-bundle": "node scripts/verify-runtime-validation-bundle.mjs",
    "prepare": "is-ci || husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:bypass": "playwright test",
    "test:e2e:working": "playwright test e2e/tests/smoke.spec.ts e2e/tests/basic-search.spec.ts e2e/tests/example-working.spec.ts",
    "test:e2e:ci": "playwright test e2e/tests/smoke.spec.ts e2e/tests/example-working.spec.ts e2e/tests/my-lessons.spec.ts",
    "contract:export": "python ../backend/scripts/export_openapi.py",
    "contract:generate": "openapi-typescript ../backend/openapi/openapi.json -o types/generated/api.d.ts --export-type",
    "contract:pull": "npm run contract:export && npm run contract:generate && npm run typecheck",
    "contract:pull:all": "npm run contract:export && npm run contract:generate && npm run typecheck",
    "contract:check": "node scripts/contract-check.mjs",
    "audit:contract": "node scripts/contract-audit.mjs",
    "audit:contract:ci": "node scripts/contract-audit.mjs --ci --json .artifacts/contract-adoption.json --md .artifacts/contract-adoption.md",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "size": "size-limit",
    "playwright:install": "playwright install",
    "verify:public-env": "node scripts/verify-public-env.mjs",
    "audit:deadcode": "node scripts/run-knip.mjs",
    "audit:deadcode:ci": "npm run audit:deadcode",
    "audit:typecov": "NODE_OPTIONS=--max-old-space-size=6144 type-coverage -p tsconfig.type-coverage.json --detail --strict --at-least 99 --ignore-as-assertion --ignore-type-assertion --ignore-non-null-assertion --ignore-files \".*/app/\\(auth\\)/student/dashboard/page\\.tsx\" --ignore-files \".*/lib/api/bookings\\.ts\"",
    "audit:typecov:ci": "NODE_OPTIONS=--max-old-space-size=6144 type-coverage -p tsconfig.type-coverage.json --detail --strict --at-least 99 --ignore-as-assertion --ignore-type-assertion --ignore-non-null-assertion --ignore-files \".*/app/\\(auth\\)/student/dashboard/page\\.tsx\" --ignore-files \".*/lib/api/bookings\\.ts\"",
    "type:smoke": "tsd --files \"type-tests/**/*.test-d.ts\"",
    "type:smoke:ci": "npm run type:smoke",
    "audit:lhci": "cross-env NEXT_PUBLIC_API_BASE=https://example.invalid next build && lhci autorun || true"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stripe/react-stripe-js": "^4.0.0",
    "@stripe/stripe-js": "^7.9.0",
    "@tanstack/react-query": "^5.85.9",
    "@tanstack/react-query-devtools": "^5.85.9",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.542.0",
    "motion": "^12.23.12",
    "next": "15.5.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "1.55.x",
    "@size-limit/file": "^11.2.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "dependency-cruiser": "^17.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "knip": "^5.63.1",
    "openapi-typescript": "7.9.1",
    "size-limit": "^11.2.0",
    "tailwindcss": "4.1.12",
    "ts-morph": "^22.0.0",
    "ts-prune": "^0.10.3",
    "tsd": "^0.31.2",
    "type-coverage": "^2.29.7",
    "typescript": "^5",
    "vercel": "^47.0.2",
    "zod": "^3.25.76"
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/*.js",
      "limit": "550 kB"
    }
  ],
  "typeCoverage": {
    "atLeast": 95,
    "ignoreFiles": [
      "^types/generated/.*$",
      "^e2e/.*$",
      "^\\.next/.*$",
      ".*__tests__/.*",
      ".*\\.test\\.ts$",
      ".*\\.test\\.tsx$"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "!types/generated/**": []
  }
}
