{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "audit:deps": "depcruise --config dependency-cruiser.config.cjs --ts-config tsconfig.json app components features hooks lib | jq '.'",
    "audit:deps:ci": "depcruise --config dependency-cruiser.config.cjs --ts-config tsconfig.json app components features hooks lib --output-type err",
    "audit:exports": "ts-prune -p tsconfig.json -i \"(^\\.next/|^e2e/|^types/generated/|\\.d\\.ts$|^playwright\\.config\\.ts$|^next\\.config\\.ts$|^proxy\\.ts$|^app/|^components/|^hooks/|^legacy-patterns/)\" | grep -v 'used in module' | grep -v -f ts-prune-allowlist.txt || true",
    "audit:exports:ci": "mkdir -p .artifacts && ts-prune -p tsconfig.json -i \"(^\\.next/|^e2e/|^types/generated/|\\.d\\.ts$|^playwright\\.config\\.ts$|^next\\.config\\.ts$|^proxy\\.ts$|^app/|^components/|^hooks/|^legacy-patterns/)\" | grep -v 'used in module' | grep -v -f ts-prune-allowlist.txt | tee .artifacts/ts-prune.txt; test ! -s .artifacts/ts-prune.txt",
    "dev": "next dev",
    "dev:test": "next dev -p 3100",
    "dev:http": "rm -rf .next && NEXT_PUBLIC_API_BASE=http://localhost:8000 next dev",
    "dev:https": "rm -rf .next && cross-env NEXT_PUBLIC_API_BASE=https://localhost:8001 NEXT_PUBLIC_APP_URL=https://localhost:3001 node server.js",
    "dev-lhci": "next start -p 3100",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ci": "eslint . --max-warnings=0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit",
    "typecheck:strict:log": "npm run typecheck:strict 2>&1 | tee .artifacts/ts-noUncheckedIndexedAccess.log",
    "typecheck:strict-all": "tsc -p tsconfig.strict-all.json --noEmit",
    "typecheck:strict-all:log": "npm run typecheck:strict-all 2>&1 | tee .artifacts/ts-strict-all.log",
    "typecheck:strict-all:fast": "tsc -p tsconfig.strict-all.json --noEmit --pretty false --incremental --tsBuildInfoFile .cache/tsbuildinfo.strict",
    "build:log": "mkdir -p .artifacts && bash -o pipefail -c \"NEXT_TELEMETRY_DISABLED=1 next build 2>&1 | tee .artifacts/next-build.log\"",
    "build:warn-check": "node scripts/check-next-warnings.js",
    "verify:runtime-validation-bundle": "node scripts/verify-runtime-validation-bundle.mjs",
    "prepare": "is-ci || husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:bypass": "playwright test",
    "test:e2e:working": "playwright test e2e/tests/smoke.spec.ts e2e/tests/basic-search.spec.ts e2e/tests/example-working.spec.ts",
    "test:e2e:ci": "playwright test e2e/tests/smoke.spec.ts e2e/tests/example-working.spec.ts e2e/tests/my-lessons.spec.ts",
    "contract:export": "python ../backend/scripts/export_openapi.py",
    "contract:generate": "openapi-typescript ../backend/openapi/openapi.json -o types/generated/api.d.ts --export-type",
    "contract:pull": "npm run contract:export && npm run contract:generate && npm run typecheck",
    "contract:pull:all": "npm run contract:export && npm run contract:generate && npm run typecheck",
    "contract:check": "node scripts/contract-check.mjs --minify",
    "contract:write:min": "node scripts/contract-check.mjs --write --minify",
    "audit:contract": "node scripts/contract-audit.mjs",
    "audit:contract:ci": "node scripts/contract-audit.mjs --ci --json .artifacts/contract-adoption.json --md .artifacts/contract-adoption.md",
    "api:types": "node ./scripts/generate-openapi-types.mjs",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "e2e:local:profile": "CI_LOCAL_E2E=1 npx playwright test frontend/e2e/profile.service-areas.spec.ts --reporter=line --trace on-first-retry",
    "e2e:local:places": "CI_LOCAL_E2E=1 npx playwright test frontend/e2e/profile.preferred-places.spec.ts --reporter=line --trace on-first-retry",
    "e2e:local:places:live": "E2E_APP_ORIGIN=http://localhost:3100 E2E_API_ORIGIN=http://localhost:8000 CI_LOCAL_E2E=1 npx playwright test frontend/e2e/profile.preferred-places.spec.ts --reporter=line --trace on-first-retry",
    "size": "size-limit",
    "playwright:install": "playwright install",
    "verify:public-env": "node scripts/verify-public-env.mjs",
    "audit:deadcode": "node scripts/run-knip.mjs",
    "audit:deadcode:ci": "npm run audit:deadcode",
    "audit:typecov": "NODE_OPTIONS=--max-old-space-size=6144 type-coverage -p tsconfig.type-coverage.json --detail --strict --at-least 99 --ignore-as-assertion --ignore-type-assertion --ignore-non-null-assertion --ignore-files \".*/app/\\(auth\\)/student/dashboard/page\\.tsx\" --ignore-files \".*/lib/api/bookings\\.ts\"",
    "audit:typecov:ci": "NODE_OPTIONS=--max-old-space-size=6144 type-coverage -p tsconfig.type-coverage.json --detail --strict --at-least 99 --ignore-as-assertion --ignore-type-assertion --ignore-non-null-assertion --ignore-files \".*/app/\\(auth\\)/student/dashboard/page\\.tsx\" --ignore-files \".*/lib/api/bookings\\.ts\"",
    "type:smoke": "tsd --files \"type-tests/**/*.test-d.ts\"",
    "type:smoke:ci": "npm run type:smoke",
    "audit:lhci": "cross-env NEXT_PUBLIC_API_BASE=https://example.invalid next build && lhci autorun || true",
    "lhci": "lhci autorun --config=lhci.config.js || true",
    "api:schema": "python ../backend/scripts/export_openapi.py",
    "api:generate": "orval --config orval.config.ts && prettier --write src/api/generated",
    "api:sync": "npm run api:schema && npm run api:generate",
    "api:lint": "spectral lint ../backend/openapi/openapi.json -r ../backend/openapi/spectral.yaml"
  },
  "dependencies": {
    "@100mslive/roomkit-react": "^0.4.2",
    "@marsidev/react-turnstile": "^1.4.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@sentry/nextjs": "^10.38.0",
    "@size-limit/preset-app": "^12.0.0",
    "@stripe/react-stripe-js": "^5.5.0",
    "@stripe/stripe-js": "^8.6.1",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/otel": "^2.1.0",
    "@vercel/speed-insights": "^1.3.1",
    "botid": "^1.5.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.563.0",
    "motion": "^12.34.0",
    "next": "^16.1.6",
    "next-axiom": "^1.10.0",
    "react": "^19.1.1",
    "react-dom": "^19.2.4",
    "react-is": "^19.2.4",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.1",
    "@jest/globals": "^30.1.2",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "1.58.2",
    "@size-limit/file": "^12.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^25.2.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.54.0",
    "cross-env": "^10.0.0",
    "dependency-cruiser": "^17.3.6",
    "eslint": "^9",
    "eslint-config-next": "^16.1.5",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.1.2",
    "jsdom": "^28.0.0",
    "knip": "^5.82.1",
    "openapi-typescript": "7.10.1",
    "orval": "^8.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "size-limit": "^12.0.0",
    "tailwindcss": "4.2.1",
    "ts-morph": "^27.0.2",
    "ts-prune": "^0.10.3",
    "tsd": "^0.33.0",
    "type-coverage": "^2.29.7",
    "typescript": "^5",
    "zod": "^4.3.6"
  },
  "size-limit": [
    {
      "name": "chunks-total-js",
      "path": ".next/static/chunks/*.js",
      "limit": "2.7 MB",
      "running": false
    },
    {
      "name": "chunks-total-css",
      "path": ".next/static/chunks/*.css",
      "limit": "50 kB",
      "running": false
    },
    {
      "name": "turbopack-runtime",
      "path": ".next/static/chunks/turbopack-*.js",
      "limit": "10 kB",
      "running": false
    }
  ],
  "typeCoverage": {
    "atLeast": 95,
    "ignoreFiles": [
      "^types/generated/.*$",
      "^e2e/.*$",
      "^\\.next/.*$",
      ".*__tests__/.*",
      ".*\\.test\\.ts$",
      ".*\\.test\\.tsx$"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "!types/generated/**": []
  },
  "overrides": {
    "path-to-regexp": "6.3.0",
    "tar-fs": "3.1.1",
    "esbuild": "0.27.1",
    "tmp": "0.2.5",
    "glob": "11.1.0",
    "minimatch": "10.2.1",
    "rimraf": "6.1.3",
    "nanoid": "5.1.6",
    "test-exclude": {
      "glob": "7.2.3",
      "minimatch": "3.1.2"
    },
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-compose-refs": "1.1.2",
    "@100mslive/roomkit-react": {
      "react": "$react"
    }
  }
}
