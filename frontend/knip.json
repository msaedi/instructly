{
  "$schema": "https://unpkg.com/knip/schema.json",
  "project": ["**/*.{js,jsx,ts,tsx}"],
  "paths": {
    "@/*": ["./*"],
    "@/components/*": ["./components/*"]
  },
  "entry": [
    "app/**/page.tsx",
    "app/**/layout.tsx",
    "components/**/*.{ts,tsx}",
    "features/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "services/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "contexts/**/*.{ts,tsx}",
    "scripts/**",
    "e2e/**"
  ],
  "ignore": ["types/reference/**","reference/dormant/**","**/*.stories.@(ts|tsx)","examples/**"],
  "ignoreBinaries": ["jq","python","lint-staged","spectral"],
  "ignoreDependencies": [
    "@tanstack/react-query-devtools",
    "eslint-config-next",
    "tailwindcss",
    "ts-prune",
    "vercel"
  ],
  "include": ["dependencies", "unlisted", "binaries", "unresolved"]
}
